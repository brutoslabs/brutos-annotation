<?xml version="1.0" encoding="UTF-8"?>
<controllers xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns='http://www.brutosframework.com.br/schema/controllers'
	xsi:schemaLocation='http://www.brutosframework.com.br/schema/controllers http://www.brutosframework.com.br/schema/controllers/brutos-controllers-1.1.xsd'>

	<controller id="/controller"
		class="org.brandao.brutos.helper.any.app1.Test10AnyController">

		<bean name="decimalProperty" target="org.brandao.brutos.helper.any.app1.DecimalProperty">
			<property name="name" />
			<property name="length" />
			<property name="decimals" />
		</bean>

		<bean name="setProperty" target="org.brandao.brutos.helper.any.app1.SetProperty">
			<property name="name" />
			<property name="values">
				<bean target="java.util.ArrayList">
					<element type="java.lang.String" />
				</bean>
			</property>
		</bean>

		<property name="property1">
			<bean target="org.brandao.brutos.helper.any.app1.Test5AnyBean">

				<constructor-arg bean="propertyC">
					<any bean="propertyType3" type="java.lang.String">
						<meta-value value="decimal" bean="decimalProperty" />
						<meta-value value="set" bean="setProperty" />
					</any>
				</constructor-arg>

				<property name="property1" bean="propertyA">
					<any bean="propertyType" type="java.lang.String">
						<meta-value value="decimal" bean="decimalProperty" />
						<meta-value value="set" bean="setProperty" />
					</any>
				</property>

				<property name="property2" bean="propertyB">
					<any bean="propertyType2" type="java.lang.String">
						<meta-value value="decimal" bean="decimalProperty" />
						<meta-value value="set" bean="setProperty" />
					</any>
				</property>

			</bean>
		</property>

		<property name="property2" bean="propertyD">
			<any bean="propertyType4" type="java.lang.String">
				<meta-value value="decimal" bean="decimalProperty" />
				<meta-value value="set" bean="setProperty" />
			</any>
		</property>

		<action id="/test" executor="testAction">
			<parameter bean="propertyE">
				<any bean="propertyType5" type="java.lang.String">
					<meta-value value="decimal" bean="decimalProperty" />
					<meta-value value="set" bean="setProperty" />
				</any>
			</parameter>
		</action>
	</controller>

</controllers>
